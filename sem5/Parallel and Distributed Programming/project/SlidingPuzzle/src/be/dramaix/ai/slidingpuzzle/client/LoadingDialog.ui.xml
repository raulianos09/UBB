<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main {
			padding: 10px;
		}
	</ui:style>
	<g:PopupPanel modal="true" animationEnabled="true"
		glassEnabled="true" autoHideEnabled="false" width="300px" height="50px">
		<g:HTMLPanel styleName="{style.main}">
			<div class='msg-popup-panel'>
				<img src='/img/loading.gif' />
				<div>Searching optimal solution ...</div>
			</div>
		</g:HTMLPanel>
	</g:PopupPanel>
</ui:UiBinder>